{
  "name": "@cakewalk-ai/api",
  "version": "1.0.0",
  "description": "Official Cakewalk API client with built-in caching",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cakewalk",
    "aeo",
    "blog",
    "api",
    "ai-search",
    "content",
    "seo"
  ],
  "author": "Cakewalk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cakewalk-ai/api"
  },
  "homepage": "https://cakewalk.ai",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18"
  }
}
